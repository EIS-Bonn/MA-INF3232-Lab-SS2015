###  http://www.semanticweb.org/eislab/ontologies/2015/2/family-ontology#hasChild

:hasChild rdf:type owl:ObjectProperty ;
          
          rdfs:domain [ rdf:type owl:Restriction ;
                        owl:onProperty :hasChild ;
                        owl:onClass :Person ;
                        owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger
                      ] .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.semanticweb.org/eislab/ontologies/2015/2/family-ontology#Female

:Female rdf:type owl:Class ;
        
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :hasChild ;
                              owl:onClass :Person ;
                              owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger
                            ] ;
        
        rdfs:subClassOf :Person ;
        
        owl:disjointWith :Male .



###  http://www.semanticweb.org/eislab/ontologies/2015/2/family-ontology#Male

:Male rdf:type owl:Class ;
      
      rdfs:subClassOf :Person .



###  http://www.semanticweb.org/eislab/ontologies/2015/2/family-ontology#Person

:Person rdf:type owl:Class .





