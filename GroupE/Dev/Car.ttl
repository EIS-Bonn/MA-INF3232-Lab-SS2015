@prefix mv: <http://eccenca.com/mobivoc/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

mv:Car
	a rdfs:Class , owl:Class ;
	rdfs:comment "A wheeled, self-powered motor vehicle used for transportation." ;
	rdfs:label "car"@en , "Auto"@de .
	
mv:FuelCar
	rdfs:subClassOf mv:Car ;
	rdfs:comment "A car which runs with fuel." ;
	rdfs:label "FuelCar"@en , "KraftstoffAuto"@de .
	
mv:BatteryElectricCar
	rdfs:subClassOf mv:Car ;
	rdfs:comment "A type of electric car that uses chemical energy stored in rechargeable battery packs." ;
	rdfs:label "BatteryElectricCar"@en , "BatterieElektroAuto"@de .

mv:PlugInHybridElectricCar
	rdfs:subClassOf mv:Car ;
	rdfs:comment "A type of hybrid car and electric car that combines a conventional internal combustion engine (ICE) propulsion system with an electric propulsion system" ;
	rdfs:label "PlugInHybridElectricCar"@en , "PlugInHybridElektroAuto"@de .


	