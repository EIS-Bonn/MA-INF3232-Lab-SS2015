@prefix mv: <http://eccenca.com/mobivoc/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix g1: <http://www.nothing.org/graph13#>.

g1:Fuel rdf:type rdfs:Class.
g1:Payment rdf:type rdfs:Class.
g1:Fuel g1:Cost g1:Payment.
	
	
mv:Currency
	a rdf:Property , owl:DatatypeProperty ;
	rdfs:comment "Indicates the currency of the fuel's cost" ;
	rdfs:label "Currency"@en , "WÃ¤hrung"@de ;
	rdfs:domain mv:Cost ;
	rdfs:range rdfs:Literal . 
	
mv:PaymentMethod
	a rdf:Property , owl:DatatypeProperty ;
	rdfs:comment "Indicates the payment methods" ;
	rdfs:label "Payment Method"@en , "Zahlungsarten"@de ;
    rdfs:domain mv:Payment ;
	rdfs:range rdfs:Literal .                           
                             
                             
